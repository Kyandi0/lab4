<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.reflection.CreatorController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="27.0" prefWidth="329.0">
         <children>
            <TextField fx:id="class_name" text="com.example.reflection." />
            <Button fx:id="create" mnemonicParsing="false" onAction="#onCreate" text="Create Object" />
            <Button fx:id="save" mnemonicParsing="false" onAction="#onSave" text="Save Changes" />
         </children>
      </HBox>
      <BorderPane prefHeight="292.0" prefWidth="437.0">
         <bottom>
            <TextArea fx:id="log" editable="false" prefHeight="76.0" prefWidth="437.0" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <VBox fx:id="content" prefHeight="160.0" prefWidth="437.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</VBox>
